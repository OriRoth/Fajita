rm -rf bin
rm -rf src
mkdir bin
mkdir -p src/test/java/org/spartan/fajita/revision/examples/usage
cp -r ../target/classes/* bin/
rm -r bin/org/spartan/fajita/revision/examples/usage/

target=src/test/java/org/spartan/fajita/revision/examples/usage/Exp2.java
rm -rf out/graphs/testcases/Exp2
mkdir -p out/graphs/testcases/Exp2
echo "" > $target
echo "" > out/graphs/testcases/Exp2/javac.csv
echo "" > out/graphs/testcases/Exp2/ecj.csv
python compilation_exp.py 14 "javac" "javac -cp bin/ -d junk/ $target"  "ecj" "ecj -1.8 -cp bin/ -d junk/ $target" # "gcj" "gcj -cp bin/ -d junk/ $target"
gnuplot -e "filename='out/graphs/testcases/Exp2/javac.csv'" graph > out/graphs/testcases/Exp2/javac.png
gnuplot -e "filename='out/graphs/testcases/Exp2/ecj.csv'" graph > out/graphs/testcases/Exp2/ecj.png

