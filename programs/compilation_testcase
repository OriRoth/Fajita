rm -rf bin
rm -rf src
mkdir bin
mkdir -p src/test/java/org/spartan/fling/revision/examples/usage
cp -r ../target/classes/* bin/
rm -r bin/org/spartan/fling/revision/examples/usage/
cp ../revision/src/test/java/org/spartan/fling/revision/examples/usage/$1.java src/test/java/org/spartan/fling/revision/examples/usage/

target=src/test/java/org/spartan/fling/revision/examples/usage/$1.java
rm -rf out/graphs/testcases/$1
mkdir -p out/graphs/testcases/$1
echo "" > out/graphs/testcases/$1/$1.csv
python compilation_testcase.py "out/graphs/testcases/$1/$1.csv" "javac" "javac -cp bin/ -d junk/ $target"  "ecj" "ecj -1.8 -cp bin/ -d junk/ $target" # "gcj" "gcj -cp bin/ -d junk/ $target"
gnuplot -e "filename='out/graphs/testcases/$1/$1.csv'" bars > out/graphs/testcases/$1/$1.png

